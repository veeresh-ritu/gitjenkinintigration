pipeline{
agent any
  stages{
    stage('compile stage'){
	 steps{
	 withmaven(maven :'mave _3_3_0_603')
	  sh ' mvn clean compile'
	}
	
  }
  }
  
  
  stages{
    stage('testing stage'){
	steps{
	 withmaven(maven :'mave _3_3_0_603')
	  sh ' mvn test'
	}
	}
	
  }
  stages{
    stage('testing stage'){
	steps{
	 withmaven(maven :'mave _3_3_0_603')
	  sh ' mvn deploy'
	}
	
  }

}
}